{
    "version": 2,
    "builds": [{
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "dist"
        }
    }],
    "routes": [{
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/assets/(.*)",
            "headers": {
                "Content-Type": "application/javascript"
            },
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*\\.js)",
            "headers": {
                "Content-Type": "application/javascript"
            },
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}